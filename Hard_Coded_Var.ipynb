   "source": [
    "# Quadratic solution example with hard-coded values\n",
    "\n",
    "import math\n",
    "\n",
    "# Coefficients\n",
    "a = 1\n",
    "b = -3\n",
    "c = 2\n",
    "\n",
    "# Calculate the discriminant\n",
    "d = b**2 - 4*a*c\n",
    "\n",
    "# Find two solutions\n",
    "sol1 = (-b - math.sqrt(d)) / (2*a)\n",
    "sol2 = (-b + math.sqrt(d)) / (2*a)\n",
    "\n",
    "print(f\"The solutions are {sol1} and {sol2}\")\n"
   ]
  }
 ],
